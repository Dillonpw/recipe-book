generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recipe {
  id          Int      @id @default(autoincrement())
  title       String   @db.VarChar(255)
  ingredients String[]
  steps       String[]
  tags        String[]
  image       String?
}
